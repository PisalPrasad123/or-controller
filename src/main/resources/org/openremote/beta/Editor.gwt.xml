<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">

<module rename-to="Editor">

    <!-- ##################################################################### -->

    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.logging.Logging"/>

    <inherits name="org.fusesource.restygwt.RestyGWT"/>

    <inherits name="com.ait.lienzo.Lienzo"/>

    <!-- ##################################################################### -->

    <set-property name="gwt.logging.logLevel" value="FINEST"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>

    <!-- ##################################################################### -->

    <source path="shared"/>
    <source path="client/shared"/>
    <source path="client/editor"/>
    <source path="client/flowdesigner"/>

    <!-- ##################################################################### -->

    <entry-point class="org.openremote.beta.client.editor.EditorEntryPoint"/>

</module>